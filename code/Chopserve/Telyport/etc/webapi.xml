<?xml version="1.0" ?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
	<route method="GET" url="/V1/telyport/gettelyport">
		<service class="Chopserve\Telyport\Api\GetTelyportManagementInterface" method="getTelyport"/>
		<resources>
			<resource ref="anonymous"/>
		</resources>
	</route>
	<route method="POST" url="/V1/telyport/telyportapi">
		<service class="Chopserve\Telyport\Api\TelyportApiManagementInterface" method="telyportApi"/>
		<resources>
			<resource ref="anonymous"/>
		</resources>
	</route>
</routes>
